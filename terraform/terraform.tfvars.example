# Copy to terraform.tfvars and set your values
# Naming: pratyush_<resource_name>_<region>_<type_of_resource>

project_name   = "apple-retail"
environment    = "dev"
aws_region     = "us-east-1"

# S3 bucket: must be globally unique (use hyphens in bucket name per AWS)
# Example: pratyush-datalake-us-east-1-dev-<account_id>
pratyush_s3_bucket_name = "pratyush-datalake-us-east-1-dev-123456789012"

pratyush_glue_job_script_bucket = "pratyush-datalake-us-east-1-dev-123456789012"
pratyush_glue_job_script_key    = "scripts"

pratyush_glue_worker_type       = "G.1X"
pratyush_glue_number_of_workers = 2
